{
  "name": "cv",
  "version": "1.0.0",
  "description": "CV, implemented in Express.js, Handlebars, HTML and CSS.",
  "main": "app.js",
  "watch": {
    "pdf": {
      "patterns": [
        "."
      ],
      "extensions": "css,html",
      "delay": 2000
    }
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "watch": "npm-watch",
    "pdf": "prince cv.html"
  },
  "author": "Thomas Wittek",
  "devDependencies": {
    "npm-watch": "^0.13.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "minireset.css": "^0.0.7",
    "nunjucks": "^3.2.4"
  }
}
